{
  "name": "ahuth.github.io",
  "version": "4.0.0",
  "private": true,
  "description": "Personal homepage.",
  "author": "Andrew Huth",
  "homepage": "https://ahuth.github.io",
  "scripts": {
    "build": "URL_ROOT=\"$PWD/build/\" ts-node -T ./scripts/build",
    "build:prod": "URL_ROOT=https://ahuth.github.io ts-node -T ./scripts/build",
    "clean": "rm -rf ./build",
    "deploy": "gh-pages -b master -d build -t",
    "lint": "eslint --ext .js,.ts scripts/",
    "prebuild": "yarn run clean",
    "predeploy": "yarn run build:prod",
    "types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^3.12.5",
    "@types/markdown-it": "^10.0.3",
    "@types/node": "^14.14.8",
    "@types/nunjucks": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "date-fns": "^2.16.1",
    "eslint": "^7.13.0",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "highlight.js": "^10.3.2",
    "js-yaml": "^3.14.0",
    "markdown-it": "^12.0.2",
    "nunjucks": "^3.2.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "zod": "^1.11.10"
  }
}
